# Frontend Environment Variables
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000
VITE_APP_NAME=Meta You
VITE_APP_VERSION=1.0.0

# Development settings
VITE_DEV_MODE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Feature flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_ACHIEVEMENTS=true
VITE_ENABLE_REAL_TIME=true

# Critical API Keys (Add your actual keys)
VITE_OPENAI_API_KEY=your-openai-api-key-here
VITE_SUPABASE_URL=your-supabase-url-here
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key-here

# Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=your-ga-id-here
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_MIXPANEL_TOKEN=your-mixpanel-token-here

# Social Authentication
VITE_GOOGLE_CLIENT_ID=your-google-client-id-here
VITE_GITHUB_CLIENT_ID=your-github-client-id-here

# Real-time Features
VITE_PUSHER_APP_KEY=your-pusher-app-key-here
VITE_PUSHER_CLUSTER=your-pusher-cluster-here

# CDN & Storage
VITE_AWS_S3_BUCKET=your-s3-bucket-here
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-name-here

# Feature Toggles
VITE_ENABLE_BETA_FEATURES=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_PERFORMANCE_MONITORING=true